    "mocha": "^5.2.0",
    "nodemailer": "^4.7.0",
    "nyc": "^13.1.0",
    "pg": "^7.7.1",
    "swagger-ui-express": "^4.0.2",
    "validator": "^10.9.0",
    "volleyball": "^1.5.1"
